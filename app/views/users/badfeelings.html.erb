<section class="feel">
  <img class="feel-img" src="data:image/png;base64,">
  <div class="feel-container">
    <h1 class="feel-title"><i class="far fa-user" style="padding-top: 3vh;"></i> <%= @current_user.name %> さんの　<i class="far fa-frown" style="color:blue"></i> 調子の悪い日 <i class="far fa-frown" style="color:blue"></i></h1>
  </div>
  <img class="feel-img" src="data:image/png;base64,">
</section>
<section class="feel-records">
  <div class="records-inner">
    <% @badfeelings.each do |badfeeling| %>
      <div class="record">
      <a></a>
        <div class="record-top">
          <div class="record-top-left">
            
            <%= Record.find(badfeeling.record_id).created_at.strftime('%Y/%m/%d %H:%M') %>
          </div>
          <div class="record-top-right">
            <%= link_to("/records/#{badfeeling.record_id}/edit", {method: "get"}) do %>
              <i class="far fa-edit" style="padding-right: 8px;"></i></a>
            <% end %>
            <%= link_to("/records/#{badfeeling.record_id}", {method: "delete"}) do %>
              <i class="far fa-trash-alt"></i>
            <% end %>
            
          </div>  
        </div>
        <div class="record-bottom">
          <div class="record-bottom-left">
            食事内容 :　<%= Record.find(badfeeling.record_id).content %>
          </div>
          <div class="record-bottom-right">
            <% if Goodfeeling.find_by(user_id: @current_user.id, record_id: Record.find(badfeeling.record_id).id) %>
              <%= link_to("/goodfeelings/#{badfeeling.record_id}/destroy", {method: "post", class: "link"}) do %>
                <%# <span class="far fa-laugh-beam like-btn-unlike1"><b>feelgood!</b></span> %>
                <i class="far fa-laugh-beam like-btn-unlike"></i><b>feelgood!</b>
              <% end %>
            <% else %>
              <%= link_to("/goodfeelings/#{badfeeling.record_id}/create", {method: "post", class: "link"}) do %>
                <%# <span class="far fa-thumbs-up like-btn"><b>feelgood?</b></span> %>
                <i class="far fa-thumbs-up like-btn"></i><b>feelgood?</b>
              <% end %>
            <% end %>

            <% if Badfeeling.find_by(user_id: @current_user.id, record_id: Record.find(badfeeling.record_id).id) %>
              <%= link_to("/badfeelings/#{badfeeling.record_id}/destroy", {method: "post", class: "link"}) do %>
                <%# <span class="far fa-tired like-btn-unlike"><b>feelbad!</b></span> %>
                <i class="far fa-tired unlike-btn-unlike" style="padding-left: 8px;"></i><b>feelbad!</b>
              <% end %>
            <% else %>
              <%= link_to("/badfeelings/#{badfeeling.record_id}/create", {method: "post", class: "link"}) do %>
                <i class="far fa-thumbs-down like-btn" style="padding-left: 8px;"></i><b>feelbad?</b>
              <% end %>
            <% end %>
          </div>
          
        </div>
        
      </div>
    <% end %>
    <%= paginate @badfeelings %>
  </div>
</section>  