<div class='side-bar'>
        <div class="logo">
          <img src="data:image/png;base64," class="logo-img">
        </div>
        <div class="icon-wrapper">
        <% if @current_user %>
          <% if current_page?(home_path) %>
            <div class='home'>
              <a href="/home">
                <i class="fas fa-home fa-2x" style="color: deeppink;"><font style="color: deeppink;">HOME</font></i>
              </a>  
            </div>
          <% else %>
            <div class='home'>
              <a href="/home">
                <i class="fas fa-home fa-2x"><font>HOME</font></i>
              </a>  
            </div>
          <% end %>
          <% if current_page?(goodfeelings_user_path(@current_user.id)) %>
            <div class='goodfeeling'>
              <%= link_to("/users/#{@current_user.id}/goodfeelings") do %>
              <%# <a href="/users/#{@current_user}/goodfeelings"> %>
                <i class="far fa-laugh-beam fa-2x" style="color: #d42a54;"><font style="color: #d42a54;">feel good!!</font></i>
              <%# </a>   %>
              <% end %>
            </div>
          <% else %>
            <div class='goodfeeling'>
              <%= link_to("/users/#{@current_user.id}/goodfeelings") do %>
              <%# <a href="/users/#{@current_user}/goodfeelings"> %>
                <i class="far fa-laugh-beam fa-2x"><font>feel good!!</font></i>
              <%# </a>   %>
              <% end %>
            </div>
          <% end %>
          <% if current_page?(badfeelings_user_path(@current_user.id)) %>
            <div class='badfeeling'>
              <%= link_to("/users/#{@current_user.id}/badfeelings") do %>
                <i class="far fa-frown fa-2x" style="color: blue;"><font style="color: blue;">feel bad ⤵︎</font></i>
              <% end %>
            </div>
          <% else %>
            <div class='badfeeling'>
              <%= link_to("/users/#{@current_user.id}/badfeelings") do %>
                <i class="far fa-frown fa-2x"><font>feel bad ⤵︎</font></i>
              <% end %>
            </div>
          <% end %>
          <% if current_page?(edit_user_path(@current_user.id)) %>
            <div class='user'>
              <%= link_to("/users/#{@current_user.id}/edit") do %>
                <i class="fas fa-user-edit fa-2x" style="color: deeppink;"><font style="color: deeppink;">Profile</font></i>
              <% end %>
            </div>
          <% else %>
            <div class='user'>
              <%= link_to("/users/#{@current_user.id}/edit") do %>
                <i class="fas fa-user-edit fa-2x"><font>Profile</font></i>
              <% end %>
            </div>
          <% end %>
          <% if current_page?("hoge") %>
            <div class='setting'>
              <%= link_to("") do %>
                <i class="fa fa-gear fa-2x" style="color: deeppink;"><font style="color: deeppink;">Setting</font></i>
              <% end %>
            </div>
          <% else %>
            <div class='setting'>
              <%= link_to("") do %>
                <i class="fa fa-gear fa-2x"><font>Setting</font></i>
              <% end %>
            </div>
          <% end %>
          <% if current_page?("hoge") %>
            <div class='logout'>
              <%= link_to("/users/#{@current_user.id}/logout", {method: :post}) do %>
                <i class="fas fa-sign-out-alt fa-2x" style="color: deeppink;"><font style="color: deeppink;">Log out</font></i>
              <% end %>
            </div>
          <% else %>
            <div class='logout'>
              <%= link_to("/users/#{@current_user.id}/logout", {method: :post}) do %>
                <i class="fas fa-sign-out-alt fa-2x"><font>Log out</font></i>
              <% end %>
            </div>
          <% end %>
        <% else %> 
          <% if current_page?(login_path) || current_page?(root_path) %> 
            <div class='login'>
              <%= link_to("/login") do %>
                <i class="fas fa-sign-in-alt fa-2x" style="color: deeppink;"><font style="color: deeppink;">Log in</font></i>
              <% end %>
            </div>
          <% else %>
            <div class='login'>
              <%= link_to("/login") do %>
                <i class="fas fa-sign-in-alt fa-2x" ><font>Log in</font></i>
              <% end %>
            </div>
          <% end %>
          <% if current_page?(signup_path) %>
            <div class='signup'>
              <%= link_to("/signup") do %>
                <i class="fas fa-user-plus fa-2x" style="color: deeppink;"><font style="color: deeppink;">Sign up</font></i>
              <% end %>
            </div>
          <% else %>
            <div class='signup'>
              <%= link_to("/signup") do %>
                <i class="fas fa-user-plus fa-2x"><font >Sign up</font></i>
              <% end %>
            </div>
          <% end %>
        <% end %>
        </div>  
      </div>